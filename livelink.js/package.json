{
  "name": "@3dverse/livelink",
  "version": "0.8.9",
  "description": "",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "typings": "./dist/sources/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "node ci/auto-generate.js ../node_modules/ ci/ ./_prebuild/",
    "build": "node esbuild.js && tsc",
    "lint": "eslint sources _prebuild",
    "clean": "rimraf _prebuild dist docs docs-md",
    "clean:docs": "rimraf docs docs-md",
    "dev": "run-p dev:*",
    "dev:esbuild": "node esbuild.js dev",
    "dev:tsc": "tsc -w",
    "predocs": "run-p clean:docs",
    "docs": "run-p typedoc typedoc:md",
    "postdocs": "prettier --write ./docs-md",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typedoc": "typedoc",
    "typedoc:md": "typedoc --plugin typedoc-plugin-markdown --out docs-md",
    "postversion": "cross-env-shell \"git commit -am v$npm_package_version && git tag -a v$npm_package_version -m v$npm_package_version\""
  },
  "license": "ISC",
  "devDependencies": {
    "@3dverse/livelink-camera-controls": "^0.3.3",
    "@3dverse/livelink.core": "^1.0.6",
    "@eslint/js": "^9.17.0",
    "@types/webxr": "^0.5.20",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.24.0",
    "eslint": "^9.17.0",
    "gl-matrix": "^3.4.3",
    "globals": "^15.14.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "threejs-math": "^0.147.0",
    "typedoc": "^0.27.6",
    "typedoc-plugin-markdown": "^4.4.1",
    "typedoc-plugin-mermaid": "^1.12.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  }
}
