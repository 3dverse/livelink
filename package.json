{
  "name": "livelink",
  "version": "1.0.0",
  "main": "livelink.js",
  "workspaces": [
    "livelink.js",
    "livelink.react",
    "livelink.samples"
  ],
  "scripts": {
    "=============== Build =================": "",
    "prebuild": "npm run clean",
    "build:js": "npm -C livelink.js run build",
    "build:react": "npm -C livelink.react run build",
    "build": "npm-run-all build:*",
    "clean:js": "npm -C livelink.js run clean",
    "clean:react": "npm -C livelink.react run clean",
    "clean": "npm-run-all --parallel clean:*",
    "=============== Dev =================": "",
    "predev": "npm run build",
    "dev:js": "npm -C livelink.js run dev",
    "dev:react": "npm -C livelink.react run dev",
    "dev:samples": "npm -C livelink.samples run dev",
    "dev": "npm-run-all --parallel dev:*",
    "dev-core": "npm link @3dverse/livelink.core && cross-env LIVELINK_CORE_URL=http://localhost:3000/index.mjs npm run dev",
    "=============== Test =================": "",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5"
  }
}
