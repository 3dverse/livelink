{
  "name": "livelink",
  "version": "1.0.0",
  "main": "livelink.js",
  "scripts": {
    "=============== Install =================": "",
    "setup:core": "cd livelink.core && npm install",
    "setup:js": "cd livelink.js && npm install",
    "setup:samples": "cd livelink.samples && npm install",
    "postinstall": "npm-run-all setup:*",
    "=============== Build =================": "",
    "prebuild": "npm run clean",
    "build:core": "npm -C livelink.core run build",
    "build:js": "npm -C livelink.js run build",
    "build": "npm-run-all build:*",
    "clean:core": "npm -C livelink.core run clean",
    "clean:js": "npm -C livelink.js run clean",
    "clean": "npm-run-all --parallel clean:*",
    "=============== Dev =================": "",
    "predev": "npm run build",
    "dev:core": "npm -C livelink.core run dev",
    "dev:js": "npm -C livelink.js run dev",
    "dev:samples": "npm -C livelink.samples run dev",
    "dev": "npm-run-all --parallel dev:*",
    "=============== Test =================": "",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "prettier": "^3.2.5"
  }
}
